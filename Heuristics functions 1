    public static float GetDistanceEuclidean(Node nodeA, Node nodeB)
    {
        float dx = Mathf.Pow(nodeA.x - nodeB.x, 2);
        float dy = Mathf.Pow(nodeA.y - nodeB.y, 2);
        float H = Mathf.Sqrt(dx + dy);

        return H;
    }

    /// <summary>
    /// Calculates distance between two nodes using Euclidean no squareroot
    /// </summary>
    /// <param name="nodeA"></param>
    /// <param name="nodeB"></param>
    /// <returns>Float value of distance between nodeA and nodeB</returns>
    public static float GetDistanceEuclideanNoSqr(Node nodeA, Node nodeB)
    {
        float dx = Mathf.Pow(nodeA.x - nodeB.x, 2);
        float dy = Mathf.Pow(nodeA.y - nodeB.y, 2);
        float H = dx + dy;

        return H;
    }
